<%= form_for @user do |f| %>

<%=f.label :username %>
<%= f.text_field :username %>
<br>
<br>
<%=f.label :password %>
<%= f.password_field :password %>
<br>
<br>
<%=f.label :name %>
<%= f.text_field :name %>
<br>
<br>
<%=f.label :email %>
<%= f.text_field :email %>
<br>
<br>
<%=f.label :age %>
<%= f.text_field :age %>
<br>
<br>
<%= f.submit submit_text %>
<% end %>
