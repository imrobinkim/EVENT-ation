<h2>New User</h2>

<%= form_for @user do |f| %>

<%=f.label :username %>
<%= f.text_field :username %>

<%=f.label :password %>
<%= f.text_field :password %>

<%=f.label :name %>
<%= f.text_field :name %>

<%=f.label :email %>
<%= f.text_field :email %>

<%=f.label :age %>
<%= f.text_field :age %>

<%= f.submit "Sign Up" %>
<% end %>
