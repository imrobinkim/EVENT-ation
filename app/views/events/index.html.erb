<div class="jumbotron">

<h1>All Events</h1>
<%= form_tag interests_path, method: :get do %>
  <%= submit_tag "Sort by Interest Category" %>
<% end %>

<div class="outside">


  <% @events.each do |event|%>
  <ul>
     <li><%= link_to event.name, event%></li>
  </ul>
  <% end %>
</div>
</div>

</div>
